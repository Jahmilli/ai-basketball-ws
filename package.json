{
  "name": "ai-basketball-ws",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint -c .eslintrc.js src",
    "lint:fix": "eslint -c .eslintrc.js '*/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "jest --testPathIgnorePatterns .st.test. --coverage --verbose",
    "test:st": "USE_REAL_REDIS=true jest --testPathIgnorePatterns '\\.(int|unit)\\.test\\.' --coverage --verbose --runInBand",
    "test:all": "jest --coverage --verbose",
    "start": "node dist/index.js",
    "start:dev": "NODE_ENV=dev ts-node src/index.ts",
    "package": "tar czf ${npm_package_name}.tar.gz Dockerfile config dist node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jahmilli/ai-basketball-ws.git"
  },
  "private": true,
  "bugs": {
    "url": "https://github.com/Jahmilli/ai-basketball-ws/issues"
  },
  "homepage": "https://github.com/Jahmilli/ai-basketball-ws#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "aws-sdk": "^2.715.0",
    "busboy": "^0.3.1",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "got": "^11.5.1",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/busboy": "^0.2.3",
    "@types/config": "^0.0.36",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/hapi__joi": "^17.1.4",
    "@types/jest": "^25.2.3",
    "@types/node": "^12.12.47",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fastify": "^3.0.3",
    "husky": "^2.7.0",
    "jest": "^25.5.4",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint && npm audit && npm test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "eslint --fix"
    ]
  }
}
